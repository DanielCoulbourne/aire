{
  "name": "glhd/aire",
  "description": "Modern Laravel form builder. Remembers old input, retrieves error messages and comes with beautiful Tailwind-based markup out of the box.",
  "keywords": [
    "laravel",
    "forms",
    "tailwind",
    "aire"
  ],
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://www.cmorrell.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "illuminate/support": "5.5.*|5.6.*|5.7.*",
    "illuminate/view": "5.5.*|5.6.*|5.7.*",
    "illuminate/events": "5.5.*|5.6.*|5.7.*",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "^1.1",
    "php-coveralls/php-coveralls": "^2.1",
    "guzzlehttp/guzzle": "~6.0",
    "symfony/css-selector": "^4.1",
    "symfony/dom-crawler": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Galahad\\Aire\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Galahad\\Aire\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Galahad\\Aire\\Support\\AireServiceProvider"
      ],
      "aliases": {
        "Aire": "Galahad\\Aire\\Support\\Facades\\Aire"
      }
    }
  }
}
